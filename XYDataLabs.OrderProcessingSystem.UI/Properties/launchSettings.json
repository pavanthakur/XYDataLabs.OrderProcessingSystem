{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "USE_HTTPS": "false",
        "SHAREDSETTINGS_PATH": "..\\Resources\\Configuration\\sharedsettings.local.json"
      },
      "applicationUrl": "http://localhost:5012"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "USE_HTTPS": "true",
        "SHAREDSETTINGS_PATH": "..\\Resources\\Configuration\\sharedsettings.local.json"
      },
      "applicationUrl": "https://localhost:5013"
    },
    "docker-dev-http": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoProfile -ExecutionPolicy Bypass -Command \"& '$(SolutionDir)Resources\\Docker\\start-docker.ps1' -Environment dev -Profile http\"",
      "workingDirectory": "$(SolutionDir)",
      "launchBrowser": true,
      "launchUrl": "http://localhost:5022",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "docker-dev-https": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoProfile -ExecutionPolicy Bypass -Command \"& '$(SolutionDir)Resources\\Docker\\start-docker.ps1' -Environment dev -Profile https\"",
      "workingDirectory": "$(SolutionDir)",
      "launchBrowser": true,
      "launchUrl": "https://localhost:5023",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "docker-uat-http": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoProfile -ExecutionPolicy Bypass -Command \"& '$(SolutionDir)Resources\\Docker\\start-docker.ps1' -Environment uat -Profile http\"",
      "workingDirectory": "$(SolutionDir)",
      "launchBrowser": true,
      "launchUrl": "http://localhost:5032",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Staging"
      }
    },
    "docker-uat-https": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoProfile -ExecutionPolicy Bypass -Command \"& '$(SolutionDir)Resources\\Docker\\start-docker.ps1' -Environment uat -Profile https\"",
      "workingDirectory": "$(SolutionDir)",
      "launchBrowser": true,
      "launchUrl": "https://localhost:5033",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Staging"
      }
    },
    "docker-prod-http": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoProfile -ExecutionPolicy Bypass -Command \"& '$(SolutionDir)Resources\\Docker\\start-docker.ps1' -Environment prod -Profile http\"",
      "workingDirectory": "$(SolutionDir)",
      "launchBrowser": true,
      "launchUrl": "http://localhost:5042",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    },
    "docker-prod-https": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-NoProfile -ExecutionPolicy Bypass -Command \"& '$(SolutionDir)Resources\\Docker\\start-docker.ps1' -Environment prod -Profile https\"",
      "workingDirectory": "$(SolutionDir)",
      "launchBrowser": true,
      "launchUrl": "https://localhost:5043",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    }
  },
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:5012",
      "sslPort": 5013
    }
  }
}
