  "profiles": {
    "http": {
      "commandName": "Executable",
      "executablePath": "powershell.exe",
      "commandLineArgs": "-Command \"cd '$(SolutionDir)'; .\\set-local-env.ps1 -ProjectType API -Profile http; Start-Sleep 2; cd '.\\XYDataLabs.OrderProcessingSystem.API'; dotnet run --launch-profile http-direct\"",
